{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://our.job.com/jobGet.schema.json",
    "type": "object",
    "properties": {
        "title": {
            "type": "string",
            "minLength": 1,
            "maxLength": 30
        },
        "salary": {
            "type": "integer",
            "minimum": 0
        },
        "equity": {
            "type": "string",
            "pattern": "0|(0?\\.[0-9]+)"
        },
        "companyHandle": {
            "type": "string",
            "minLength": 1,
            "maxLength": 10
        }
    },
    "additionalProperties": false,
    "required": []
}